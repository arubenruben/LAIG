<lxs>
    <scene root="terrain" axis_length="5"/>

    <views default="frontSideCamera">
        <perspective id="frontSideCamera" near="0.1" far="500" angle="45">
            <from x="7" y="10" z="30" />
            <to x="7" y="0" z="0" />
        </perspective>

         <perspective id="backSideCamera" near="0.1" far="500" angle="45">
            <from x="7" y="10" z="-10" />
            <to x="7" y="0" z="20" />
        </perspective>

          <perspective id="leftSideCamera" near="0.1" far="500" angle="45">
            <from x="-10" y="10" z="10" />
            <to x="14" y="0" z="10" />
        </perspective>

        <ortho id="topOrtho" near="0.1" far="500" angle="45" left="-15" right="15" top="15" bottom="-15">
            <from x="7" y="50" z="10" />
            <to x="7" y="0" z="10" />
            <up x="0.0" y="0.0" z="1.0" />
        </ortho>

        <perspective id="rightSideCamera" near="0.1" far="500" angle="45">
            <from x="24" y="10" z="10" />
            <to x="0" y="0" z="10" />
        </perspective>


        <perspective id="topPerspective" near="0.1" far="500" angle="45">
            <from x="8" y="20" z="10" />
            <to x="7" y="0" z="10" />
        </perspective>

        <perspective id="billboardCamera" near="0.1" far="500" angle="45">
            <from x="7" y="5" z="7" />
            <to x="7" y="3" z="0" />
        </perspective>

        <perspective id="poolCamera" near="0.1" far="500" angle="45">
            <from x="4" y="4" z="20" />
            <to x="7" y="0.5" z="10" />
        </perspective>

        <perspective id="chairCamera" near="0.1" far="500" angle="45">
            <from x="2.0" y="1.5" z="9.5" />
            <to x="7" y="0" z="11" />
        </perspective>

       
    </views>

    <globals>
        <ambient r="0.3" g="0.3" b="0.3" a="1.0" />
        <background r="0.0" g="0.0" b="0.0" a="1.0" />
    </globals>


   
    <lights>
        <omni id="meio_dia" enabled="0">
            <location x="7" y="5" z="10" w="1.0" />
            <ambient r="0.7" g="0.7" b="0.7" a="1.0" />
            <diffuse r="0.7" g="0.7" b="0.7" a="1.0" />
            <specular r="0.7" g="0.7" b="0.7" a="1.0" />
            <attenuation constant ="1.0" linear = "0" quadratic = "0.0"/>
        </omni>
        
        <omni id="por_do_sol" enabled="1">
            <location x="7" y="5" z="10" w="1.0" />
            <ambient r="0.9" g="0.5" b="0" a="1.0" />
            <diffuse r="0.9" g="0.5" b="0" a="1.0" />
            <specular r="0.9" g="0.5" b="0" a="1.0" />
            <attenuation constant ="1.0" linear = "0" quadratic = "0.0"/>
        </omni>


        <spot id="spotLightBillboard1" enabled="0" angle="90" exponent="5">
            <location x="5" y="3" z="2" w="1.0" />
            <target x="4" y= "5" z="0"/>
            <ambient r="0" g="0" b="0" a="1.0" />
            <diffuse r="1" g="1" b="1" a="1.0" />
            <specular r="1" g="1" b="1" a="1.0" />
            <attenuation constant ="0" linear = "0.5" quadratic = "0.0"/>
        </spot>

        <spot id="spotLightBillboard12" enabled="0" angle="90" exponent="5">
            <location x="9" y="3" z="2" w="1.0" />
            <target x="9" y= "4" z="0"/>
            <ambient r="0" g="0" b="0" a="1.0" />
            <diffuse r="1" g="1" b="1" a="1.0" />
            <specular r="1" g="1" b="1" a="1.0" />
            <attenuation constant ="0" linear = "0.5" quadratic = "0.0"/>
        </spot>


        <spot id="spot_esperguicadeira" enabled="0" angle="90" exponent="1">
            <location x="9" y="5" z="12" w="1.0" />
            <target x="13" y= "1" z="12"/>
            <ambient r="0" g="0" b="0" a="1.0" />
            <diffuse r="1" g="1" b="1" a="1.0" />
            <specular r="1" g="1" b="1" a="1.0" />
            <attenuation constant ="1" linear = "0" quadratic = "0.0"/>
        </spot>




    </lights>

    <textures>
        <texture id="demoTexture" file="scenes/images/vidral.jpg" />
        <texture id="dirt" file="scenes/images/dirt.jpg" />
        <texture id="grass" file="scenes/images/grass.jpg" />
        <texture id="marble" file="scenes/images/marble.jpg" />
        <texture id="esperguicadeira" file="scenes/images/esperguicadeira.png"/>
        <texture id="water" file="scenes/images/water.jpg" />
        <texture id="pool_tiles" file="scenes/images/pool_tiles.jpg" />
        <texture id="plastic" file="scenes/images/plastic.png"/>
        <texture id="colmo" file= "scenes/images/colmo.png"/>
        <texture id="tronco" file= "scenes/images/madeira.jpg"/>
        <texture id="vice_city" file="scenes/images/vice_city.jpg"/>
        <texture id="ola" file="scenes/images/sumol.jpg"/>
        <texture id="iron" file="scenes/images/iron.jpg"/>
        <texture id="palm_tree_leaf" file="scenes/images/palm_tree_leaf.jpg"/>
        <texture id="beachBall" file="scenes/images/beachBall.jpg"/>
        <texture id="beachBuoy1" file="scenes/images/beachBuoy1.jpg"/>
        <texture id="beachBuoy2" file="scenes/images/beachBuoy2.jpg"/>
        <texture id="fences" file="scenes/images/fences.jpg"/>

    </textures>

    <materials>
        <material id="demoMaterial" shininess="10">
            <emission r="0.0" g="0.0" b="0.0" a="1.0" />
            <ambient r="1.0" g="1.0" b="1.0" a="1.0" />
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />
        </material>
        <material id="orange_material" shininess="10">
            <emission r="0.0" g="0.0" b="0.0" a="1.0" />
            <ambient r="1.0" g="0.5" b="0.0" a="1.0" />
            <diffuse r="1.0" g="0.5" b="0.0" a="1.0" />
            <specular r="1.0" g="0.5" b="0.0" a="1.0" />
        </material>

        <material id="red_material" shininess="10">
            <emission r="0" g="0" b="0" a="1.0" />
