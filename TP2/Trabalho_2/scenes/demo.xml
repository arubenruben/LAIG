    </materials>

    <transformations>


        <!-- VERIFICADAS-->
        <transformation id="demoTransform">
            <translate x="0" y="0" z="0" />
        </transformation>


        <transformation id="identity">
            <translate x="0" y="0" z="0"/>
        </transformation>

        <transformation id="underground_transf">
            <translate x="0" y="-2" z="0" />
            <rotate axis="x" angle="90" />
            <scale x="14" y="20" z="1" />

        </transformation>

        <transformation id ="terrain_transf">
            <translate x="0" y="0" z="0"/>
        </transformation>

        <transformation id="spot_pub_transf">
            <translate x="4.7" y="2" z="0.5"/>
        </transformation>

    </transformations>

     <animations>    
        <animation id = "first">
            <keyframe instant = "100">
                <translate x = "0" y = "50" z = "0"/>
                <rotate angle_x="0" angle_y="0" angle_z="0"/>
                <scale x="1" y="1" z="1"/>
            </keyframe>

             <keyframe instant = "4000">
                <translate x = "0" y = "20" z = "0"/>
                <rotate angle_x="0" angle_y="360" angle_z="0"/>
                <scale x="1.2" y="1.2" z="1.2"/>
            </keyframe>


             <keyframe instant = "2000000">
                <translate x = "0" y = "0" z = "0"/>
                <rotate angle_x="0" angle_y="0" angle_z="0"/>
                <scale x="1" y="1" z="1"/>
            </keyframe>
        </animation>

    </animations>

    <primitives>
        <primitive id="square">
            <rectangle x1="0" y1="0" x2="1" y2="1" />
        </primitive>

        <primitive id="cilinder_pau">
            <cylinder base="0.4" top="0.5" height="1" slices="10" stacks="10" />
        </primitive>

        <primitive id="cilinder_spot">
            <cylinder base="0.1" top="0.1" height="4" slices="15" stacks="15" />
        </primitive>

        <primitive id="torus_bouy">
            <torus inner="0.2" outer="0.5" slices="30" loops="30"/>
        </primitive>

        <primitive id="sphere_ball">
            <sphere radius="0.3" slices="30" stacks="30"/>
        </primitive>

        <primitive id="cilinder_guarda_sol">
            <cylinder base="0.5" top="0" height="1" slices="15" stacks="15" />
        </primitive>

        <primitive id="cilinder_perna_esperguicadeira">
            <cylinder base="0.3" top="0.3" height="1" slices="10" stacks="10" />
        </primitive>

        <primitive id="triangle">
            <triangle x1="0" y1="0" z1="0" x2="1" y2="1" z2="0" x3="2" y3="0" z3="0" />
        </primitive>

        <primitive id="triangle_fence_primitive">
            <triangle x1="0" y1="0" z1="0" x2="0.25" y2="0.25" z2="0" x3="0.5" y3="0" z3="0" />
        </primitive>

           <primitive id="sphere_palm_tree">
            <sphere radius="0.1" slices="10" stacks="10"/>
        </primitive>

        <primitive id="cilinder_palm_tree">
            <cylinder base="0.1" top="0.1" height="2" slices="10" stacks="10" />
        </primitive>


    </primitives>

    <components>

        
        <component id="square">
            <transformation>
                <translate x="0" y="0" z="0"/>
            </transformation>
            <animationref id="first"/>
            <materials>
                <material id="demoMaterial"/>
            </materials>
            <texture id="beachBuoy2" length_s="1" length_t="1"/>
            <children>
                <primitiveref id="square"/>
            </children>
        </component>


        <!-- TERRAIN ITSELF / ROOT OF XML-->

        <component id="terrain">
            <transformation>
                <transformationref id="identity" />
            </transformation>
            <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="dirt" length_s="0.5" length_t="0.5"/>
            <children>
               <!-- <componentref id="underground"/>
                <componentref id="floor"/>
                <componentref id="sides"/>
                <componentref id="pool"/>
                <componentref id= "fence_core"/>
               <componentref id="spot_pub"/>-->

                <componentref id = "square"/>



            </children>
        </component>


    </components>
</lxs>









